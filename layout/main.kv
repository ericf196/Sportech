#:kivy 1.9.0
# -*- coding: utf-8 -*-

<PopupForm>
    BoxLayout
        Popup:
            id: popupForm
            size_hint: (None, None)
            title_align: "center"
            title_size: "20sp"
            size: 400, 200
            pos_hint: {'center_x': 0.5, 'y': 0.4}
            BoxLayout:
                orientation: "vertical"
                padding: 12
                spacing: 12
                Image:
                    source: "resources/background/anterior.png"
                Label:
                    id: labelPopup
                    #size_hint_y: 50
                Button:
                    text: "Cerrar"
                    #size_hint_y: 25
                    on_press: root.close_popup()
<MainForm>:
    canvas.before:
        Color:
            rgba: 0, 0, 0, 1
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout
        orientation: "vertical"
        padding: 12
        spacing: 12
        size: root.size
        BoxLayout:
            orientation: "horizontal"
            padding: 12
            spacing: 12
            size_hint: 1, .5
            #Body
            ActionBar:
                pos_hint: {'top':1}
                ActionView:
                    use_separator: True
                    ActionPrevious:
                        app_icon: 'resources/icons/Logo_Sportech37_magenta_mini_only.png'
                        with_previous: False
                    ActionOverflow:
                    ActionGroup:
                        text: '   Engarde   '
                        mode: 'spinner'
                        ActionButton:
                            text: ' Conectar '
                            on_press: root.action_bar_connection_to_engarde()
                        ActionButton:
                            text: ' Desconectar '
                            on_press: root.action_bar_disconnect_connection_to_engarde()
                        ActionButton:
                            text: ' Combate\n Siguiente '
                            on_press: root.change_combat_in_engarde(1)
                        ActionButton:
                            text: ' Combate\n Anterior '
                            on_press: root.change_combat_in_engarde(2)
                        ActionButton:
                            text: ' Invertir \n Competidores '
                    ActionGroup:
                        text: '   Videoref   '
                        mode: 'spinner'
                        ActionButton:
                            text: '  Conectar  '
                            on_press: root.action_bar_connection_to_videoref()
                        ActionButton:
                            text: '  Desconectar  '
                            on_press: root.action_bar_disconnect_connection_to_videoref()
                        ActionButton:
                            text: '  Coordinate  '
                            on_press: root.send_coordinates_videoref()
                        ActionButton:
                            text: '  Hello  '
                            on_press: root.send_hello_videoref()
                        ActionButton:
                            text: '  Disp  '
